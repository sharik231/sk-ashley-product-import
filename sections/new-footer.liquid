<footer class="site-footer" aria-label="Footer" role="contentinfo" style="--top-bg:{{section.settings.color_footer_bg}};--top-text:{{section.settings.color_footer_text}};--top-border:{{section.settings.color_border_top}};--btm-bg:{{section.settings.color_fb_bg}};--btm-text:{{section.settings.color_fb_text}};--btm-border:{{section.settings.color_fb_border}};--footer-link-color:{{section.settings.color_footer_link}};--footer-link-color-hover:{{section.settings.color_footer_link_hover}};--btm-link-color:{{section.settings.color_btm_link}};--btm-link-color-hover:{{section.settings.color_btm_link_hover}};">
  
  <div class="footer-top">
     <div class="container footer-top-wrap">
        <div class="footer-top-col">

            {% if section.settings.footer-logo != nil %}
                <div class="footer-top-col2">
              {% render 'image-shopify', screenSize:'third', shopImg:section.settings.footer-logo, alt:section.settings.col4_text, classes:'w-full' %} 
                  </div>
                  {%- endif -%}
                  
       
        </div>
       <div class="footer-top-col">
              {%- if section.settings.text != blank -%}
                <div class="footer-top__item-text">
                  {{ section.settings.text }}
                </div>
              {%- endif -%}
          
              {%- if section.settings.title != blank -%}
                <h4 class="footer-top-nw-col-title">
                  <img src="https://cdn.shopify.com/s/files/1/0729/7466/3936/files/Icon.png?v=1732355186"/><span>{{ section.settings.title | escape }}</span>
                </h4>
             
              {%- endif -%}

              <div class="footer-top__newsletter newsletter">
                {%- assign formId = 'ContactFooter' -%}
                {% form 'customer', id: formId, novalidate: 'novalidate' %}
                  <input type="hidden" name="contact[tags]" value="newsletter">
                  <div class="newsletter-input-group {% if form.errors %} newsletter-input-group--error{% endif %}">
                    <input type="email" name="contact[email]" id="{{ formId }}-email" class="sfmc-email newsletter-input-group__input{% if form.errors %} newsletter-input-group__error{% endif %} border border-gray-200"
                      value="{{ form.email }}" placeholder="{{ 'general.newsletter_form.email_placeholder' | t }}" aria-label="{{ 'general.newsletter_form.email_placeholder' | t }}" aria-required="true"
                      required autocorrect="off" autocapitalize="off"
                      {% if form.errors %}
                        aria-invalid="true"
                        aria-describedby="{{ formId }}-email-error"
                        data-form-status
                      {% endif %}
                    >
                    <button type="submit" class="newsletter__submit theme-btn theme-btn--black" name="commit" id="Subscribe" >
                      <span>{{ section.settings.newsletter_signup }}</span>
                    </button>
                  </div>

                  {%- if form.posted_successfully? -%}
                    <span class="newsletter_signup--success block mt-2 p-0 rounded bg-green-200 text-green-500" tabindex="-1" data-form-status >
                      {{ section.settings.newsletter_signup_success }}
                    </span>
                  {%- endif -%}

                  {% if form.errors contains 'email' %}
                    <span id="{{ formId }}-email-error" class="input-error-message block mt-2 p-0 rounded bg-red-200 text-red-500" >
                      <span class="sr-only">{{ 'general.accessibility.error' | t }} </span>
                      <span class="site-footer__newsletter-error">{{ section.settings.newsletter_signup_error }}</span>
                    </span>
                  {% endif %}
                {% endform %}
              </div>

        </div>
    </div>   
    <div class="container footer-top__wrapper">
      {%- for block in section.blocks -%}
        <div class="footer-top__col">
          {%- case block.type -%}
         
   {%- when 'text' -%}
              {%- if block.settings.title -%}
                <h4 class="footer-top__col-title">
                  <span>{{ block.settings.title | escape }}</span>
                </h4>
              {%- endif -%}

              {%- if block.settings.description -%}
                <p class="footer-top__item-description">{{ block.settings.description }}</p>
              {%- endif -%}

              {%- if block.settings.text != blank -%}
                <div class="footer-top__item-text">
                  {{ block.settings.text }}
                </div>
              {%- endif -%}
            {%- when 'link_list' -%}
              {%- assign footer_linklist = block.settings.menu -%}
              <h4 class="footer-top__col-title">
                <span>{{ block.settings.title | escape }}</span>
              </h4>
              <rec-footer-links>
                <template slot id="title">
                  <h1>{{ block.settings.title | escape }}</h1>
                </template>
                <ul>
                  {%- for link in linklists[footer_linklist].links -%}
                    <li class="site-footer__linklist--links-link">
                      <a
                        href="{{ link.url }}"
                        {% if link.current %}
                          aria-current="page"
                        {% endif %}
                      >
                        {{ link.title | escape }}
                      </a>
                    </li>
                  {%- endfor -%}
                </ul>
              </rec-footer-links>
          {%- endcase -%}
        </div>
      {%- endfor -%}
    </div>
  </div>
  <div class="footer-bottom">
          <div class="container footer-bottom-wrap">
            <div class="footer-bottom-contact">
{%- if settings.footer-contact != '' -%}
        <img src="https://cdn.shopify.com/s/files/1/0729/7466/3936/files/Vector_1.png?v=1732361367"/> <a href="tel:{{ settings.footer-contact }}" target="_blank">
       {{ settings.footer-contact }}
         </a>
        {%- endif -%}
            </div>
        <div class="footer-bottom__social-icons">
       {%- if settings.social_twitter_link != '' -%}
         <a href="{{ settings.social_twitter_link }}" target="_blank">
       <i class="fa fa-twitter" aria-hidden="true"></i>
         </a>
        {%- endif -%}
       {%- if settings.social_facebook_link != '' -%}
         <a href="{{ settings.social_facebook_link }}" target="_blank">
       <i class="fa fa-facebook-square" aria-hidden="true"></i>
         </a>
        {%- endif -%}
       {%- if settings.social_pinterest_link != '' -%}
         <a href="{{ settings.social_pinterest_link }}" target="_blank">
       <i class="fa fa-pinterest-square" aria-hidden="true"></i>
         </a>
        {%- endif -%}  
       {%- if settings.social_instagram_link != '' -%}
         <a href="{{ settings.social_instagram_link }}" target="_blank">
       <i class="fa fa-instagram" aria-hidden="true"></i>
         </a>
        {%- endif -%} 
       {%- if settings.social_tumblr_link != '' -%}
         <a href="{{ settings.social_tumblr_link }}" target="_blank">
       <i class="fa fa-tumblr-square" aria-hidden="true"></i>
         </a>
        {%- endif -%}    
       {%- if settings.social_snapchat_link != '' -%}
         <a href="{{ settings.social_snapchat_link }}" target="_blank">
       <i class="fa fa-snapchat-square" aria-hidden="true"></i>
         </a>
        {%- endif -%}  
       {%- if settings.social_youtube_link != '' -%}
         <a href="{{ settings.social_youtube_link }}" target="_blank">
          <i class="fa fa-youtube-square" aria-hidden="true"></i>
         </a>
        {%- endif -%}  
       {%- if settings.social_vimeo_link != '' -%}
         <a href="{{ settings.social_vimeo_link }}" target="_blank">
          <i class="fa fa-vimeo-square" aria-hidden="true"></i>
         </a>
        {%- endif -%}          
