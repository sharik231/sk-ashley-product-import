      jQuery(this).parent().prepend(jQuery(this));
      jQuery('.nav-tabs.mobile-tabs .nav-item').toggleClass('visible-xs visible-sm');
    });
  }

  if ($(window).width() < 992) {
    mobileTabDropdown();
  }

  $(window).resize(function () {
    if ($(window).width() < 992) {
      mobileTabDropdown();
    } else
    {
      jQuery('.nav-tabs.mobile-tabs .nav-item').off();
    }
  });

});
