 Arial, sans-serif;\n    max-width: 1200px;\n    margin: 0 auto;\n  }\n  \n  \/* Desktop Tab Styles *\/\n  .desktop-view .container {\n    font-family: Arial;\n  }\n  \n  .desktop-view .tab {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n  }\n  \n  .desktop-view .tab button {\n    background-color: inherit;\n    border: none;\n    outline: none;\n    cursor: pointer;\n    padding: 14px 16px;\n    transition: 0.3s;\n    width: 32%;\n    margin-bottom: 10px;\n    text-align: left;\n    border: 1px solid #ddd;\n    border-radius: 4px;\n  }\n  \n  .desktop-view .tab button:hover {\n    background-color: #f1f1f1;\n  }\n  \n  .desktop-view .tab button.active {\n    background-color: #f1f1f1;\n    border-bottom: 2px solid #555;\n  }\n  \n  .desktop-view .tabcontent {\n    display: none;\n    padding: 6px 12px;\n    border: none!important;\n  }\n  \n  .desktop-view .tabcontent iframe {\n    width: 100%;\n  }\n  \n  .desktop-view .address {\n    display: block;\n    margin-top: 8px;\n    font-size: 14px;\n    color: #555;\n  }\n  \n  \/* Mobile Accordion Styles *\/\n  .mobile-view {\n    display: none;\n  }\n  \n  .mobile-store-container {\n    max-width: 100%;\n    padding: 10px;\n  }\n  \n  .store-card {\n    border: 1px solid #ddd;\n    border-radius: 8px;\n    margin-bottom: 15px;\n    overflow: hidden;\n    box-shadow: 0 2px 5px rgba(0,0,0,0.1);\n  }\n  \n  .store-header {\n    padding: 15px;\n    position: relative;\n    cursor: pointer;\n  }\n  \n  .store-header h3 {\n    margin: 0 0 8px 0;\n    color: #333;\n    font-size: 18px;\n  }\n  \n  .store-hours {\n    display: block;\n    font-size: 14px;\n    color: #555;\n    margin-bottom: 10px;\n  }\n  \n  .address-phone {\n    margin-bottom: 12px;\n  }\n  \n  .address {\n    display: block;\n    font-size: 14px;\n    color: #333;\n    margin-bottom: 5px;\n    line-height: 1.4;\n  }\n  \n  .phone {\n    display: block;\n    font-size: 14px;\n    color: #333;\n    font-weight: bold;\n  }\n  \n  .set-store-btn {\n    background-color: #f5f5f5;\n    border: 1px solid #ddd;\n    border-radius: 4px;\n    padding: 8px 15px;\n    font-size: 14px;\n    margin-top: 10px;\n    cursor: pointer;\n  }\n  \n  .arrow-icon {\n    position: absolute;\n    right: 15px;\n    top: 15px;\n    font-size: 14px;\n    color: #666;\n    transition: transform 0.3s;\n  }\n  \n  .store-map {\n    display: none;\n    border-top: 1px solid #eee;\n  }\n  \n  .map-footer {\n    padding: 10px 15px;\n    background: #f9f9f9;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    font-size: 13px;\n  }\n  \n  .view-larger-map {\n    color: #1a73e8;\n    text-decoration: none;\n  }\n  \n  \/* Active state when map is shown *\/\n  .store-card.active .store-map {\n    display: block;\n  }\n  \n  .store-card.active .arrow-icon {\n    transform: rotate(180deg);\n  }\n  \n  \/* Responsive switching *\/\n  @media screen and (max-width: 768px) {\n    .desktop-view {\n      display: none;\n    }\n    .mobile-view {\n      display: block;\n    }\n  }\n  \n  @media screen and (min-width: 769px) {\n    .desktop-view {\n      display: block;\n    }\n    .mobile-view {\n      display: none;\n    }\n  }\n<\/style>\n\n<script>\n  \/\/ Desktop Tab Functionality\n  function openLocation(evt, locationName) {\n    var i, tabcontent, tablinks;\n    tabcontent = document.getElementsByClassName(\"tabcontent\");\n    for (i = 0; i < tabcontent.length; i++) {\n      tabcontent[i].style.display = \"none\";\n    }\n    tablinks = document.getElementsByClassName(\"tablinks\");\n    for (i = 0; i < tablinks.length; i++) {\n      tablinks[i].className = tablinks[i].className.replace(\" active\", \"\");\n    }\n    document.getElementById(locationName).style.display = \"block\";\n    evt.currentTarget.className += \" active\";\n  }\n\n  \/\/ Get the element with id=\"defaultOpen\" and click on it\n  document.getElementById(\"defaultOpen\").click();\n\n  \/\/ Mobile Accordion Functionality\n  function toggleStoreDetails(storeId) {\n    var storeCard = document.getElementById(storeId).parentNode;\n    storeCard.classList.toggle('active');\n    \n    \/\/ Close other open accordions\n    var allCards = document.querySelectorAll('.store-card');\n    allCards.forEach(function(card) {\n      if (card !== storeCard && card.classList.contains('active')) {\n        card.classList.remove('active');\n      }\n    });\n  }\n\n  \/\/ Handle window resize to ensure proper display\n  window.addEventListener('resize', function() {\n    if (window.innerWidth <= 768) {\n      document.getElementById('desktop-container').style.display = 'none';\n      document.getElementById('mobile-container').style.display = 'block';\n    } else {\n      document.getElementById('desktop-container').style.display = 'block';\n      document.getElementById('mobile-container').style.display = 'none';\n    }\n  });\n<\/script>","color_scheme":"","padding_top":16,"padding_bottom":52}}},"order":["shogun-helper","main","shogun-optimizer","custom_promo_box_RpBi3x","custom_breadcrumb_HRkJMn","custom_liquid_piC6mN"]}